<div class="body">
<h4>Wishlist</h4>
<div class="list">
  {{#if gifts}}
    <p>Last updated: {{#if last_update}}{{last_update}}{{else}}N/A{{/if}}</p>
    <ul>
      {{#each gifts}}
      <li>{{this.name}}</li>
      {{/each}}
  </ul>
  {{else}}
    Wishlist is empty.
  {{/if}}
</div>
{{#if myAccount}}
  <a href="wishlist/add">Add an item to your wishlist!</a>
{{/if}}
</div>
